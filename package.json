{
  "name": "@netu-consultants-ltd/netu-react-dsf-components",
  "version": "0.1.0",
  "description": "React Component Library Using the GOV-CY DSF Design System",
  "repository": "https://github.com/netu-consultants-ltd/netu-react-dsf-components.git",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "homepage": "https://github.com/netu-consultants-ltd/netu-react-dsf-components#readme",
  "author": "Porfyrios Vasileiou <porfyriosv@netu.com.cy>",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/netu-consultants-ltd/netu-react-dsf-components/issues"
  },
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs",
    "compile": "tsc",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "predeploy": "yarn build-storybook",
    "deploy-storybook": "set CACHE_DIR=/tmp&& gh-pages -d storybook-static",
    "version-and-publish": "run() { yarn clean $1 && yarn build $1 && version=$(cd src/$1 && npm version --no-git-tag-version $2) && (cd src/$1 && npm publish --registry=https://npm.pkg.github.com/) && git add src/$1 && git commit -m \"$1 - $version\" && git push; }; run",
    "patch": "run() { yarn version-and-publish $1 patch; }; run",
    "minor": "run() { yarn version-and-publish $1 minor; }; run",
    "major": "run() { yarn version-and-publish $1 major; }; run"
  },
  "dependencies": {
    "help": "^3.0.2",
    "multi-input-input": "^0.0.3"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@storybook/addon-essentials": "7.0.27",
    "@storybook/addon-interactions": "7.0.27",
    "@storybook/addon-links": "7.0.27",
    "@storybook/blocks": "7.0.27",
    "@storybook/react": "7.0.27",
    "@storybook/react-webpack5": "7.0.27",
    "@storybook/testing-library": "0.2.0",
    "@types/react": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "gh-pages": "^5.0.0",
    "hex-rgb": "^5.0.0",
    "postcss-import": "^15.1.0",
    "prop-types": "15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.2",
    "react-router-dom": "^6.14.2",
    "react-transition-group": "^4.4.5",
    "remark-gfm": "^3.0.1",
    "rimraf": "^5.0.1",
    "rollup": "^3.26.2",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "storybook": "7.0.27",
    "storybook-addon-react-router-v6": "^1.0.2",
    "styled-components": "^6.0.4",
    "typescript": "^5.1.6"
  }
}
